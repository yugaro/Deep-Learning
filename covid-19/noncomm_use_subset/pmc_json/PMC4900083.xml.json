    "metadata": {
        "title": "Cirrhosis is a risk factor for total hip arthroplasty for avascular necrosis",
        "authors": [
            {
                "first": "Thomas",
                "middle": [],
                "last": "Deleuran",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "S\u00f8ren",
                "middle": [],
                "last": "Overgaard",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "Hendrik",
                "middle": [],
                "last": "Vilstrup",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "Peter",
                "middle": [],
                "last": "Jepsen",
                "suffix": "",
                "email": null,
